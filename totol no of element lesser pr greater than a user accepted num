package Brainy;

import java .util.Scanner;

public class jugad
{
	public static void main(String args[])
	{
		
		int[] array= {1,4,6,22,45,67,8,20};
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter num: ");
		int num= sc.nextInt();
		
		int minCount=0;
		int maxCount=0;
		
		for(int i=0;i<array.length;i++) {
			if(array[i]<num) 
				minCount++;
			else 
				maxCount++;
			
		}
		System.out.println("total no of element less than "+num+" is "+minCount );
		System.out.println("total no of element greater than "+num+" is "+maxCount );
		
		
		
	}
}
