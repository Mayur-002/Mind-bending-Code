import java.util.*;

public class Main{
    public static void main(String[] args){
        int[] num={1,2,3,4,5};
        int sum = calculateSum(num);
        System.out.println(sum);
         }
    public static int  calculateSum(int[] arr){
       int sum=0;
        for(int i=0;i<arr.length;i++){
        sum=sum+arr[i];
        }
        return sum;
    }
}
