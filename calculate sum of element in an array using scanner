Scanner sc = new Scanner(System.in);
        System.out.println("Enter size of element: ");
        int n = sc.nextInt();
        int[] num=new int[n];
        System.out.println("Enter elements: ");
        for(int i=0;i<num.length;i++){
            num[i] = sc.nextInt();
        }
        int sum = calculateSum(num);
        System.out.println(sum);
         }
